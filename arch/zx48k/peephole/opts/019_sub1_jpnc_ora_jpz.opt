;; Tries to optimize a == b for U/Integers
;; Replace sequence:
;;  call __EQ16
;;  or a | and a
;;  jp nz, ...
;; With:
;;  or a
;;  sbc hl, de
;;  jp z, ...

OLEVEL: 1
OFLAG: 18

REPLACE {{
  call __EQ16
  $1 a
  jp $2, $3
}}

DEFINE {{
  $4 = ("z" && ($2 == "nz")) || nz
}}

IF {{
  ($1 == or) || ($1 == and)
}}

WITH {{
  or a
  sbc hl, de
  jp $4, $3
}}
