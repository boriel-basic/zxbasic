function attr(byval row as ubyte, byval col as ubyte) as ubyte
    asm
    push namespace core

    PROC
    LOCAL __ATTR_END

    ld e, (ix+7)
    ld d, (ix+5)

    
    call __IN_SCREEN
    jr nc, __ATTR_END

    call __ATTR_ADDR
    ld a, (hl)	

__ATTR_END:
    ENDP

    pop namespace
    end asm

end function














